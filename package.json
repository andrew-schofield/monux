{
  "name": "Monux",
  "version": "0.7.0",
  "description": "Unoffical Monzo Client for Desktop",
  "main": "app.js",
  "repository": "https://github.com/robjtede/monux",
  "author": "robjtede",
  "license": "MIT",
  "scripts": {
    "start:dev": "yarn electron .",
    "start": "yarn build && ./Monux-darwin-x64/Monux.app/Contents/MacOS/Monux",
    "build": "yarn electron-packager . Monux --platform darwin --icon monux --protocol monux --protocol-name Monux --overwrite"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "debug": "^2.6.8",
    "electron": "beta",
    "electron-config": "^1.0.0",
    "electron-contextmenu-middleware": "^1.0.3",
    "electron-gh-releases": "^2.0.4",
    "electron-image-menu": "^1.0.1",
    "electron-packager": "^8.7.0",
    "electron-window-state": "^4.1.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  },
  "devDependencies": {
    "eslint": "^3.19.0"
  }
}
