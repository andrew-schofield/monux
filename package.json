  },
  "dependencies": {
    "date-fns": "^1.25.0",
    "debug": "^2.6.0",
    "electron": "^1.4.14",
    "electron-config": "^0.2.1",
    "electron-contextmenu-middleware": "^1.0.2",
    "electron-gh-releases": "^2.0.4",
    "electron-image-menu": "^1.0.1",
    "electron-packager": "^8.5.0",
    "electron-window-state": "^4.0.1",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^3.13.1"
  }
}
