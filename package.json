{
  "name": "Monux",
  "version": "0.5.0",
  "description": "Unoffical Monzo Client for Desktop",
  "main": "app.js",
  "repository": "https://github.com/robjtede/monux",
  "author": "robjtede",
  "license": "MIT",
  "scripts": {
    "start:dev": "$(yarn bin)/electron .",
    "start": "yarn build && ./Monux-darwin-x64/Monux.app/Contents/MacOS/Monux",
