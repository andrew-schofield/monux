{
  "name": "Monzoo",
  "version": "0.4.0",
  "description": "Unoffical Monzo Client for Desktop",
  "main": "app.js",
  "repository": "https://github.com/robjtede/monzoo",
  "author": "robjtede",
  "license": "MIT",
  "scripts": {
    "start:dev": "$(yarn bin)/electron .",
    "start": "yarn build && ./Monzoo-darwin-x64/Monzoo.app/Contents/MacOS/Monzoo",
    "build": "$(yarn bin)/electron-packager . Monzoo --platform darwin --version 1.4.13 --protocol monzoo --protocol-name Monzoo --overwrite"
  },
  "dependencies": {
    "date-fns": "^1.22.0",
    "debug": "^2.6.0",
    "electron": "^1.4.13",
    "electron-config": "^0.2.1",
    "electron-gh-releases": "^2.0.4",
    "electron-packager": "^8.4.0",
    "electron-window-state": "^4.0.1",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^3.12.2"
  }
}
